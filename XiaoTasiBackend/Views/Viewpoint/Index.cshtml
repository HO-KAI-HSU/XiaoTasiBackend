    <script type="text/javascript">
        $(document).ready(function () {
            var loginInfo = JSON.parse(localStorage.getItem('loginInfo'));
            var managerItem = `
                <span class="hello" > Welcome!</span >
                <span class="managerName">${loginInfo.name}</span >`;
            $(".user").html(managerItem);//要刷新的div

            if ($.fn.DataTable.isDataTable('#viewpointListTable')) {
                console.log("table true");
                $('#viewpointListTable').DataTable({
                    destroy: true,
                    select: true,
                    language: {
                        emptyTable: "Not Viewpoint Datas!"
                    },
                    ajax: {
                        serverSide: true,
                        processing: true,
                        url: "@Url.Action("getViewpointList", "Viewpoint")",
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        dataType: "JSON"
                    },
                    rowId: 'viewpointId',
                    columns: [
                        { data: "viewpointId" },
                        { data: "viewpointTitle" },
                        { data: "viewpointAddress" },
                        { data: "viewpointContent" },
                        {
                            data: "viewpointPicPath", render: function (data, type, row, meta) {
                                return `<img src="${ data }">`;
                            }
                        },
                        { data: "fDate" },
                        {
                            data: "operation", render: function (data, type, row, meta) {
                                return `
                                    <a class="btn btn-info" id="editViewpoint">
									    <i class="icon-edit "></i>
								    </a>
								    <a class="btn btn-danger" id="deleteViewpoint">
									    <i class="icon-trash "></i>
								    </a>`;
                            }
                        }
                    ]
                });
            } else {
                console.log("table false");
            }


			$("#viewpointList").on('click', '#editViewpoint', function () {
                var viewpointId = $(this).closest("tr")
                    .attr('id');
                $.ajax({
                    url: "@Url.Action("getViewpointInfo", "Viewpoint")",
                    type: "POST",
                    data: JSON.stringify({ viewpointId: viewpointId}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
                    success: function (data) {
                        var viewpointInfo = data.data[0];
                        $("#viewpointInfoModal").addClass("in");
                        $(".editId").val(viewpointInfo.viewpointId);
                        $(".editViewpointTitle").val(viewpointInfo.viewpointTitle);
                        $(".editViewpointCity").val(viewpointInfo.viewpointCity);
                        $(".editViewpointArea").val(viewpointInfo.viewpointArea);
                        $(".editViewpointAddress").val(viewpointInfo.viewpointAddress);
                        $(".editViewpointContent").val(viewpointInfo.viewpointContent);
                        $("#viewpointInfoModal").attr({ "aria-hidden": "false", style: "display:block;"});
                    }
                })
            })
            $("#addViewpoint").on('click', function () {
                $("#addViewpointModal").addClass("in");
                $("#addViewpointModal").attr({ "aria-hidden": "false", style: "display:block;" });
            })
            $("#viewpointInfoModal").on('click', '#closeViewpointEdit', function(){
                $("#viewpointInfoModal").removeClass("in");
                $("#viewpointInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
            })
            $("#addViewpointModal").on('click', '#closeViewpointAdd', function(){
                $("#addViewpointModal").removeClass("in");
                $("#addViewpointModal").attr({ "aria-hidden": "true", style: "display:none;"});
            })

            $("#viewpointInfoModal").on('click', '#updateViewpoint', function () {
                var data = new FormData();
                var viewpointPicFile = $(".editViewpointPicFile").get(0).files;
                var viewpointId = $(".editId").val();
				var viewpointTitle = $(".editViewpointTitle").val();
                var viewpointCity = $(".editViewpointCity").val();
                var viewpointArea = $(".editViewpointArea").val();
                var viewpointAddress = $(".editViewpointAddress").val();
                var viewpointContent = $(".editViewpointContent").val();
                data.append("viewpointPicFile", viewpointPicFile[0]);
                data.append("viewpointId", viewpointId);
                data.append("viewpointTitle", viewpointTitle);
                data.append("viewpointCity", viewpointCity);
                data.append("viewpointArea", viewpointArea);
                data.append("viewpointAddress", viewpointAddress);
                data.append("viewpointContent", viewpointContent);
                $.ajax({
                    url: "@Url.Action("updateViewpointInfo", "Viewpoint")",
                    type: "POST",
                    data: data,
                    dataType: "JSON",
                    processData: false,
                    contentType: false,
                    success: function (data) {
                        alert(data.reason);
                        if (data.success == 1) {
                            $("#viewpointInfoModal").removeClass("in");
                            $("#viewpointInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
                            var url = '@Url.Action("Index", "Viewpoint")';
                            window.location.href = url;
                        }
                    }
                })
            })
            $("#viewpointList").on('click', '#deleteViewpoint', function(){
                var viewpointId = $(this).closest("tr")
                    .attr('id');
                $.ajax({
                    url: "@Url.Action("deleteViewpoint", "Viewpoint")",
                    type: "POST",
                    data: JSON.stringify({ viewpointId: viewpointId}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
                    success: function (data) {
                        alert(data.reason);
                        $("#viewpointInfoModal").removeClass("in");
                        $("#viewpointInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
                        var url = '@Url.Action("Index", "Viewpoint")';
                        window.location.href = url;
                    }
                })
            })

            $("#uploadBatchViewpoint").on('click', function () {
                $("#uploadBatchViewpointModal").addClass("in");
                $("#uploadBatchViewpointModal").attr({ "aria-hidden": "false", style: "display:block;" });
            })
            $("#uploadBatchViewpointModal").on('click', '#closeBatchViewpointUpload', function () {
                $("#uploadBatchViewpointModal").removeClass("in");
                $("#uploadBatchViewpointModal").attr({ "aria-hidden": "true", style: "display:none;" });
            })
            $("#addViewpointModal").on('click', '#addViewpoint', function(){
                var data = new FormData();
                var viewpointPicFile = $(".addViewpointPicFile").get(0).files;
                var viewpointTitle = $(".addViewpointTitle").val();
                var viewpointCity = $(".addViewpointCity").val();
                var viewpointArea = $(".addViewpointArea").val();
                var viewpointAddress = $(".addViewpointAddress").val();
                var viewpointContent = $(".addViewpointContent").val();
                data.append("viewpointPicFile", viewpointPicFile[0]);
                data.append("viewpointTitle", viewpointTitle);
                data.append("viewpointCity", viewpointCity);
                data.append("viewpointArea", viewpointArea);
                data.append("viewpointAddress", viewpointAddress);
                data.append("viewpointContent", viewpointContent);
                $.ajax({
                    url: "@Url.Action("addViewpoint", "Viewpoint")",
                    type: "POST",
                    data: data,
                    dataType: "JSON",
                    processData: false,
                    contentType: false,
                    success: function (data) {
                        alert(data.reason);
                        if (data.success == 1) {
                            $("#addViewpointModal").removeClass("in");
                            $("#addViewpointModal").attr({ "aria-hidden": "true", style: "display:none;"});
                            var url = '@Url.Action("Index", "Viewpoint")';
                            window.location.href = url;
                        }
                    }
                })
            })
            $("#uploadBatchViewpointModal").on('click', '#uploadBatchViewpoint', function(){
                var data = new FormData();
                var uploadBatchViewpointFile = $(".uploadBatchViewpointFile").get(0).files;
                data.append("excelFile", uploadBatchViewpointFile[0]);
                data.append("excelModule", "viewpoint");
                $.ajax({
                    url: "@Url.Action("ImportFromExcel", "ExcelBatchUpload")",
                    type: "POST",
                    data: data,
                    dataType: "JSON",
                    processData: false,
                    contentType: false,
                    success: function (data) {
                        alert(data.reason);
                        if (data.success == 1) {
                            $("#uploadBatchViewpointModal").removeClass("in");
                            $("#uploadBatchViewpointModal").attr({ "aria-hidden": "true", style: "display:none;" });
                            var url = '@Url.Action("Index", "Viewpoint")';
                            window.location.href = url;
                        }
                    }
                })
            })
        });

    </script>
