                console.log("table true");
                $('#latestMediaNewsListTable').DataTable({
                    destroy: true,
                    select: true,
                    language: {
                        emptyTable: "Not LatestMediaNews Datas!"
                    },
                    ajax: {
                        serverSide: true,
                        processing: true,
                        url: "@Url.Action("getLatestMediaNewsList","LatestMediaNews")",
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        dataType: "JSON"
                    },
                    rowId: 'latestMediaNewsId',
                    columns: [
                        { data: "latestMediaNewsId" },
                        { data: "latestMediaNewsTitle" },
                        {
                            data: "latestMediaNewsStime",
                            render: function (data, type, row, meta) {
                                var showTime = row.latestMediaNewsStime + " ~ " + row.latestMediaNewsEtime;
                                return showTime;
                            }
                        },
                        {
                            data: "url",
                            render: function (data, type, row, meta) {
                                var latestMediaNewsType = row.latestMediaNewsType;
                                var url = latestMediaNewsType == 2 ? row.latestMediaNewsMovieUrl : `<img src= ${row.latestMediaNewsPicPath} >`;
                                return url;
                            }
                        },
                        { data: "fDate" },
                        {
                            data: "operation", render: function (data, type, row, meta) {
                                return `
								    <a class="btn btn-info" href="#" id="editLatestMediaNews">
									    <i class="icon-edit "></i>
								    </a>
								    <a class="btn btn-danger" href="#" id="deleteLatestMediaNews">
									    <i class="icon-trash "></i>
								    </a>`;
                            }
                        }
                    ]
                });
            } else {
                console.log("table false");
            }

            //$.ajax({
            //    url: "@Url.Action("getLatestMediaNewsList","LatestMediaNews")",
            //    type: "Get",
            //    contentType: "application/json; charset=utf-8",
            //    dataType: "JSON",
            //    success: function (data) {
            //        var latestMediaNewsList = getLatestMediaNewsList(data.data)
            //        $("#latestMediaNewsList").html(latestMediaNewsList);//要刷新的div
            //    }
            //})

            function getLatestMediaNewsList(data) {
                let latestMediaNewsHtml = '';
                data.forEach(item => {
                    var latestMediaNewsType = item.latestMediaNewsType;
                    var showTime = item.latestMediaNewsStime + "~" + item.latestMediaNewsEtime;
                    var url = latestMediaNewsType == 2 ? item.latestMediaNewsMovieUrl : `<img src= ${item.latestMediaNewsPicPath} >`;
                    latestMediaNewsHtml += `
                        <tr>
						    <td class="center" id="latestMediaNewsRowId">${ item.latestMediaNewsId }</td>
						    <td class="center">${ item.latestMediaNewsTitle }</td>
                            <td class="center">${ showTime }</td>
                            <td class="center">${ url }</td>
                            <td class="center">${ item.fDate }</td>
							<td class="center">
								<a class="btn btn-info" href="#" id="editLatestMediaNews">
									<i class="icon-edit "></i>
								</a>
								<a class="btn btn-danger" href="#" id="deleteLatestMediaNews">
									<i class="icon-trash "></i>
								</a>
							</td>
					    </tr>`;
                });
                return latestMediaNewsHtml;
            }

            function getLatestMediaNewsTypeData(selectLatestMediaNewsType = 0) {
                let typeHtml = '';
                var selectedStr = "selected";
                var emptyStr = "";
                typeHtml += `
                    <option value="1" ${ selectLatestMediaNewsType == 1 ? selectedStr : emptyStr }>媒體圖片</option>
                    <option value="2" ${ selectLatestMediaNewsType == 2 ? selectedStr : emptyStr }>媒體影像</option>
                    `;
                return typeHtml;
            }

            function getLatestMediaNewsTypeName(latestMediaNewsType) {
                var latestMediaNewsTypeName = "";
                switch (latestMediaNewsType) {
                    case 1: {
                        latestMediaNewsTypeName = "媒體圖片";
                        break;
                    }
                    case 2: {
                        latestMediaNewsTypeName = "媒體影像";
                        break;
                    }
                    default: {
                        break;
                    }
                }
                return latestMediaNewsTypeName;
            }

            function dateTimetransfar(dateTimeStr) {
                var dateVal = new Date(dateTimeStr);
                var day = dateVal.getDate().toString().padStart(2, "0");
                var month = (1 + dateVal.getMonth()).toString().padStart(2, "0");
                var hour = dateVal.getHours().toString().padStart(2, "0");
                var minute = dateVal.getMinutes().toString().padStart(2, "0");
                var sec = dateVal.getSeconds().toString().padStart(2, "0");
                var ms = dateVal.getMilliseconds().toString().padStart(3, "0");
                var inputDate = dateVal.getFullYear() + "-" + (month) + "-" + (day) + "T" + (hour) + ":" + (minute) + ":" + (sec) + "." + (ms);
                return inputDate;
            }

            $("#latestMediaNewsList").on('click', '#editLatestMediaNews', function () {
                var latestMediaNewsId = $(this).closest("tr")
                    .attr('id');
                $.ajax({
                    url: "@Url.Action("getLatestMediaNewsInfo", "LatestMediaNews")",
                    type: "POST",
                    data: JSON.stringify({ latestMediaNewsId: latestMediaNewsId}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
