    <script type="text/javascript">
        $(document).ready(function() {
            $.ajax({
                url: "@Url.Action("getLatestNewsList","LatestNews")",
                type: "Get",
                contentType: "application/json; charset=utf-8",
                dataType: "JSON",
                success: function (data) {
                    var latestNewsList = getLatestNewsList(data.data)
                    $("#latestNewsList").html(latestNewsList);//要刷新的div
                }
            })

            function getLatestNewsList(data) {
                let latestNewsHtml = '';
                data.forEach(item => {   
                    latestNewsHtml += `
                        <tr>           
                            <td class="center" id="latestNewsRowId">${ item.latestNewsId }</td>
                            <td class="center">${ item.latestNewsTitle }</td>
                            <td class="center">${ item.latestNewsEnTitle }</td>
                            <td class="center">${ item.latestNewsContent }</td>
                            <td class="center">${ item.latestNewsEnContent }</td>
                            <td class="center"><img src="${ item.latestNewsPicPath }"></td>
                            <td class="center">${ item.fDate }</td>
                            <td class="center">
                                <a class="btn btn-info" href="#" id="editLatestNews">
                                    <i class="icon-edit "></i>                                            
                                </a>
                                <a class="btn btn-danger" href="#" id="deleteLatestNews">
                                    <i class="icon-trash "></i> 
                                </a>
                            </td>                                           
                        </tr>`;
                });
                console.log(latestNewsHtml)
                return latestNewsHtml;
            }
            $("#latestNewsList").on('click', '#editLatestNews', function(){
                var latestNewsId = $(this).closest("tr")   
                       .find("#latestNewsRowId")    
                       .text();
                $.ajax({
                    url: "@Url.Action("getLatestNewsInfo","LatestNews")",
                    type: "POST",
                    data: JSON.stringify({latestNewsId: latestNewsId}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
                    success: function (data) {
                        var latestNewsInfo = data.data;
                        $("#latestNewsInfoModal").addClass("in");
                        $(".editId").val(latestNewsInfo.latestNewsId);
                        $(".editLatestNewsName").val(latestNewsInfo.name);
                        $("#latestNewsInfoModal").attr({ "aria-hidden": "false", style: "display:block;"});
                    }
                })
            })
            $("#latestNewsInfoModal").on('click', '#closeLatestNewsEdit', function(){
                $("#latestNewsInfoModal").removeClass("in");
                $("#latestNewsInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
            })
            $("#addLatestNewsModal").on('click', '#closeLatestNewsAdd', function(){
                $("#addLatestNewsModal").removeClass("in");
                $("#addLatestNewsModal").attr({ "aria-hidden": "true", style: "display:none;"});
            })
            $("#latestNewsInfoModal").on('click', '#updateLatestNews', function(){
                var latestNewsId = $(".editId").val();
                var latestNewsTitle = $(".editLatestNewsTitle").val();
                $.ajax({
                    url: "@Url.Action("updateLatestNewsInfo","LatestNews")",
                    type: "POST",
                    data: JSON.stringify({latestNewsId: latestNewsId, latestNewsTitle: latestNewsTitle}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
                    success: function (data) {
                        $("#latestNewsInfoModal").removeClass("in");
                        $("#latestNewsInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
                        var url = '@Url.Action("Index", "LatestNews")';
                        window.location.href = url;
                        alert(data.data);
                    }
                })
            })
            $("#latestNewsList").on('click', '#deleteLatestNews', function(){
                var latestNewsId = $(this).closest("tr")   
                       .find("#latestNewsRowId")    
                       .text();
                $.ajax({
                    url: "@Url.Action("deleteLatestNews","LatestNews")",
                    type: "POST",
                    data: JSON.stringify({latestNewsId: latestNewsId}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
                    success: function (data) {
                        $("#latestNewsInfoModal").removeClass("in");
                        $("#latestNewsInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
                        var url = '@Url.Action("Index", "LatestNews")';
                        window.location.href = url;
                        alert(data.data);
                    }
                })
            })
            $("#addLatestNews").on('click', function(){
                $("#addLatestNewsModal").addClass("in");
                $("#addLatestNewsModal").attr({ "aria-hidden": "false", style: "display:block;"});
            })
            $("#addLatestNewsModal").on('click', '#addLatestNews', function(){
                var data = new FormData();
                var latestNewsPicFile = $(".addLatestNewsPicFile").get(0).files;
                var latestNewsTitle = $(".addLatestNewsTitle").val();
                var latestNewsEnTitle = $(".addLatestNewsEnTitle").val();
                var latestNewsContent = $(".addLatestNewsContent").val();
                var latestNewsEnContent = $(".addLatestNewsEnContent").val();
                var latestNewsStime = $(".addLatestNewsStime").val();
                var latestNewsEtime = $(".addLatestNewsEtime").val();
                data.append("latestNewsPicFile", latestNewsPicFile[0]);
                data.append("latestNewsTitle", latestNewsTitle);
                data.append("latestNewsEnTitle", latestNewsEnTitle);
                data.append("latestNewsContent", latestNewsContent);
                data.append("latestNewsEnContent", latestNewsEnContent);
                data.append("latestNewsStime", latestNewsStime);
                data.append("latestNewsEtime", latestNewsEtime);
                console.log(data);
                $.ajax({
                    url: "@Url.Action("addLatestNews","LatestNews")",
                    type: "POST",
                    data: data,
                    dataType: "JSON",
                    processData: false,
                    contentType: false,
                    success: function (data) {
                        $("#addLatestNewsModal").removeClass("in");
                        $("#addLatestNewsModal").attr({ "aria-hidden": "true", style: "display:none;"});
                        var url = '@Url.Action("Index", "LatestNews")';
                        window.location.href = url;
                        alert(data.data);
                    }
                })
            })
        });

    </script>
    <!-- end: JavaScript-->
</body>
</html>