    <script type="text/javascript">
        var locationArr = [];
        $(document).ready(function() {
            $.ajax({
                url: "@Url.Action("getLocationList","Location")",
                type: "Get",
                contentType: "application/json; charset=utf-8",
                dataType: "JSON",
                success: function (data) {
                    locationArr = data.data;
                    var locationInfo = getLocationSelectData();
                    $(".addLocationId").html(locationInfo);//要刷新的div
                }
            })
            $.ajax({
                url: "@Url.Action("getBoardingList","Boarding")",
                type: "Get",
                contentType: "application/json; charset=utf-8",
                dataType: "JSON",
                success: function (data) {
                    var boardingList = getBoardingList(data.data)
                    $("#boardingList").html(boardingList);//要刷新的div
                }
            })
            function getBoardingList(data) {
                let boardingHtml = '';
                data.forEach(item => {
                    var locationInfo = getLocationInfo(item.locationId);
                    boardingHtml += `
                        <tr>           
						    <td class="center" id="BoardingRowId">${ item.BoardingId }</td>
                            <td class="center">${ locationInfo.locationName }</td>
                            <td class="center"><img src="${ locationInfo.locationPicPath }"></td>
						    <td class="center">${ item.boardingDatetime }</td>
							<td class="center">
								<a class="btn btn-info" href="#" id="editBoarding">
									<i class="icon-edit "></i>                                            
								</a>
								<a class="btn btn-danger" href="#" id="deleteBoarding">
									<i class="icon-trash "></i> 
								</a>
							</td>                                           
					    </tr>`;
                });
                console.log(boardingHtml)
                return boardingHtml;
            }
            function getLocationSelectData() {
                let locationHtml = '';
                locationArr.forEach(item => {   
                    locationHtml += `
　                        <option value="${ item.locationId }">${ item.locationName }</option>                                        
					    `;
                });
                console.log(locationHtml);
                return locationHtml;
            }

            function getLocationInfo(locationId) {
                var groupInfo = '';
                locationArr.forEach(item => {   
