    <script type="text/javascript">
		var travelArr = [];
        var travelStepArr = [];
        var transportationArr = [];
		var transportationTravelArr = [];

        $(document).ready(function () {
            var loginInfo = JSON.parse(localStorage.getItem('loginInfo'));
            var managerItem = `
                <span class="hello" > Welcome!</span >
                <span class="managerName">${loginInfo.name}</span >`;
            $(".user").html(managerItem);//要刷新的div


			$(".addTravelId").change(function () {
                console.log($(this).val());
                var travelArr = JSON.parse(localStorage.getItem('travelArr'));
                for (var travelKey in travelArr) {
                    if (travelArr[travelKey].travelId == $(this).val()) {
                        travelStepArr = travelArr[travelKey].travelStepList;
                        localStorage.setItem("travelStepPartArr", JSON.stringify(travelStepArr));
                        getTravelStepList(travelStepArr);
                    }
				}
			});
            $.ajax({
                url: "@Url.Action("getTravelList", "Travel")",
                type: "Post",
                contentType: "application/json; charset=utf-8",
                dataType: "JSON",
                success: function (data) {
					travelArr = data.data;
                    travelStepPartArr = data.data[0].travelStepList;
                    localStorage.setItem("travelArr", JSON.stringify(travelArr));
                    localStorage.setItem("travelStepPartArr", JSON.stringify(travelStepPartArr));
                    var select = $(".addTravelId");
                    var travelStepArr = new Array();
                    for (var i = 0; i < travelArr.length; i++) {
                        travelStepArr.push(travelArr[i].travelStepList);
					    select.append("<option value='"+travelArr[i].travelId+"'>"
							+ travelArr[i].travelName + "</option>");
                    }
                    localStorage.setItem("travelStepArr", JSON.stringify(travelStepArr));
                    getTravelStepList();
                }
			})
            $.ajax({
                url: "@Url.Action("getTransportationList","Transportation")",
                type: "Post",
                contentType: "application/json; charset=utf-8",
                dataType: "JSON",
                success: function (data) {
                    transportationArr = data.data;
                    localStorage.setItem("transportationArr", JSON.stringify(transportationArr));
	                var select = $("#addTransportationIds");
				    for (var i = 0; i < transportationArr.length; i++) {
					    select.append("<option value='"+transportationArr[i].transportationId+"'>"
							+ transportationArr[i].transportationName + "</option>");
				    }
				    $('#addTransportationIds').selectpicker('val', '');
				    $('#addTransportationIds').selectpicker('refresh');
                }
            })

            if ($.fn.DataTable.isDataTable('#transportTravelListTable')) {
                console.log("table true");
                $('#transportTravelListTable').DataTable({
                    destroy: true,
                    select: true,
                    language: {
                        emptyTable: "Not TransportTravel Datas!"
                    },
                    ajax: {
                        serverSide: true,
                        processing: true,
                        url: "@Url.Action("getTransportTravelList","TransportTravel")",
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        dataType: "JSON"
                    },
                    rowId: 'travelTransportationMatchId',
                    columns: [
                        { data: "travelTransportationMatchId" },
                        { data: "travelName" },
                        {
                            data: "travelStepId", render: function (data, type, row, meta) {
                                var travelStepId = data;
                                var travelStep = getTravelStep(travelStepId);
                                return travelStep;
                            }
                        },
                        {
                            data: "transportationIds", render: function (data, type, row, meta) {
                                var transportationNumber = '';
                                var transportationIdArr = data.split(",");
                                for (var transpostationKey in transportationArr) {
                                    if (transportationIdArr.includes(transportationArr[transpostationKey].transportationId.toString())) {
                                        transportationNumber += transportationArr[transpostationKey].transportationLicensesNumber + ',';
                                    }
                                }
                                return transportationNumber;
                            }
                        },
                        {
                            data: "operation", render: function (data, type, row, meta) {
                                return `
								<a class="btn btn-info" href="#" id="editTransportTravel">
									<i class="icon-edit "></i>
								</a>
								<a class="btn btn-danger" href="#" id="deleteTransportTravel">
									<i class="icon-trash "></i>
								</a>`;
                            }
                        }
                    ]
                });
            } else {
                console.log("table false");
            }

            function getTravelStep(travelStepId) {
                var travelStepArr = JSON.parse(localStorage.getItem('travelStepArr'));
                var travelStep = "";
                for (var i = 0; i < travelStepArr.length; i++) {
                    for (var j = 0; j < travelStepArr[i].length; j++) {
                        if (travelStepArr[i][j].travelStepId == travelStepId) {
                            travelStep = " 第 " + (parseInt(j) + 1) + "梯次：" + travelStepArr[i][j].travelStime + " 到 " + travelStepArr[i][j].travelEtime;
                        }
                    }
                }
                return travelStep;
            }

            function getTravelStepList() {
                var travelStepArr = JSON.parse(localStorage.getItem('travelStepPartArr'));
                $('.addTravelStepId option').remove();
				var select = $(".addTravelStepId");
				for (var i = 0; i < travelStepArr.length; i++) {
					select.append("<option value='" + travelStepArr[i].travelStepId + "'>"
						+ " 第 " + (parseInt(i) + 1) + "梯次：" + travelStepArr[i].travelStime + " 到 " + travelStepArr[i].travelEtime + "</option>");
				}
            }
            function getTransportataionTravelInit(data) {
                var transportationTravelList = [];
                data.forEach(item => {
                    var transportationNumber = '';
                    var transportTravelTmp = {};
                    var transportationIdArr = item.transportationIds.split(",");
                    for (var transpostationKey in transportationArr) {
                        if (transportationIdArr.includes(transportationArr[transpostationKey].transportationId.toString())) {
                            transportationNumber += transportationArr[transpostationKey].transportationLicensesNumber  + ',';
                        }
                    }
                    transportTravelTmp.travelTransportationMatchId = item.travelTransportationMatchId;
                    transportTravelTmp.travelName = item.travelName;
                    transportTravelTmp.transportationLicensesNumber = transportationNumber;
                    transportationTravelList.push(transportTravelTmp);
                });
                return transportationTravelList;
            }

            $("#transportTravelList").on('click', '#editTransportTravel', function(){
                var transportTravelId = $(this).closest("tr")
                    .attr('id');
                console.log(transportTravelId);
                var editSelect = $("#editTransportationIds");
                var editId = '';
                var transportationArr = JSON.parse(localStorage.getItem('transportationArr'));
                for (var transportationTravelInfoKey in transportationTravelArr) {
                    if (transportationTravelArr[transportationTravelInfoKey].travelTransportationMatchId == transportTravelId) {
                        editId = transportationTravelArr[transportationTravelInfoKey].travelTransportationMatchId;
                        var transportationIdArr = transportationTravelArr[transportationTravelInfoKey].transportationIds.split(",");
                        console.log(transportationIdArr);
				        for (var i = 0; i < transportationArr.length; i++) {
                            console.log(transportationArr[i].transportationId);
					        editSelect.append("<option value='"+transportationArr[i].transportationId+"'>"
							    + transportationArr[i].transportationName + "</option>");
				        }
                        $('#editTransportationIds').selectpicker('val', transportationIdArr);
                        break;
                    }
                }
				$('#editTransportationIds').selectpicker('refresh');
                $("#transportTravelInfoModal").addClass("in");
                $(".editId").val(editId);
                $("#transportTravelInfoModal").attr({ "aria-hidden": "false", style: "display:block;"});
            })
            $("#transportTravelInfoModal").on('click', '#closeTransportTravelEdit', function(){
                $("#transportTravelInfoModal").removeClass("in");
                $("#transportTravelInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
            })
            $("#addTransportTravelModal").on('click', '#closeTransportTravelAdd', function(){
                $("#addTransportTravelModal").removeClass("in");
                $("#addTransportTravelModal").attr({ "aria-hidden": "true", style: "display:none;"});
            })
            $("#transportTravelInfoModal").on('click', '#updateTransportTravel', function(){
                var transportTravelId = $(".editId").val();
                var transportationIdArr = $("#editTransportationIds").val();
                var transportationIds = transportationIdArr.join();
                console.log(transportationIds);
                $.ajax({
                    url: "@Url.Action("updateTransportTravelInfo","TransportTravel")",
                    type: "POST",
                    data: JSON.stringify({transportTravelId: transportTravelId, transportationIds: transportationIds}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
                    success: function (data) {
                        alert(data.reason);
                        if (data.success == 1) {
                            $("#transportTravelInfoModal").removeClass("in");
                            $("#transportTravelInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
                            var url = '@Url.Action("Index", "TransportTravel")';
                            window.location.href = url;
                        }
                    }
                })
            })
            $("#transportTravelList").on('click', '#deleteTransportTravel', function(){
                var transportTravelId = $(this).closest("tr")
                    .attr('id');
                $.ajax({
                    url: "@Url.Action("deleteTransportTravel","TransportTravel")",
                    type: "POST",
                    data: JSON.stringify({transportTravelId: transportTravelId}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
                    success: function (data) {
                        alert(data.reason);
                        $("#transportTravelInfoModal").removeClass("in");
                        $("#transportTravelInfoModal").attr({ "aria-hidden": "true", style: "display:none;"});
                        var url = '@Url.Action("Index", "TransportTravel")';
                        window.location.href = url;
                    }
                })
            })
            $("#addTransportTravel").on('click', function(){
                $("#addTransportTravelModal").addClass("in");
                $("#addTransportTravelModal").attr({ "aria-hidden": "false", style: "display:block;"});
            })
            $("#addTransportTravelModal").on('click', '#addTransportTravel', function(){
				var travelId = $(".addTravelId").val();
                var travelStepId = $(".addTravelStepId").val();
                var transportationIdArr = $("#addTransportationIds").val();
                var transportationIds = transportationIdArr.join();
                $.ajax({
                    url: "@Url.Action("addTransportTravel","TransportTravel")",
                    type: "POST",
                    data: JSON.stringify({ travelId: travelId, travelStepId: travelStepId, transportationIds: transportationIds}),
                    contentType: "application/json; charset=utf-8",
                    dataType: "JSON",
                    success: function (data) {
                        alert(data.reason);
                        if (data.success == 1) {
                            $("#addTransportTravelModal").removeClass("in");
                            $("#addTransportTravelModal").attr({ "aria-hidden": "true", style: "display:none;"});
                            var url = '@Url.Action("Index", "TransportTravel")';
                            window.location.href = url;
                        }
                    }
                })
            })
        });

    </script>
    <!-- end: JavaScript-->
</body>
</html>
